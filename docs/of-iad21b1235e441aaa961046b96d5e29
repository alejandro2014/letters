					$("<div style='margin:0px 20px 20px 30px;font-size:16px;border:1px solid #d5e1e5'><strong style='padding:15px 10px 10px;display:block;border-bottom:1px solid #d5e1e5;font-size:18px;background:#f2f2f2;text-align:center'>Amplía tu formación</strong><ul id='side_courses' style='list-style-type: none;padding:0px;margin:0px'></ul>        <a href='http://www.lectiva.com/buscar.htm?" + _querystring_mas_cursos + "' style='display:block;text-align:center;padding:10px;text-decoration:none;font-size:16px;background:#f2f2f2;text-decoration:underline'> Ver más cursos</a></div>").insertAfter($("#sendMailModal").parent().parent());

