	<div><input type="hidden" name="ruxsent" id="ruxsent" value="false" /></div>
</form>
<script type="text/javascript">
	//<![CDATA[
	
	addEvt = function () {
		var listenerFunction = function (fn, e) {
			var target = e.target || e.srcElement;
			fn(e, target);
		}

		if (window.addEventListener) {
			return function(el, type, fn, capture) {
				el.addEventListener(type, function (e) {listenerFunction(fn,e)} , (!!capture));
			};
			
		} else if (window.attachEvent) {
			return function(el, type, fn) {
				el.attachEvent("on" + type, function (e) {listenerFunction(fn,e)});
			};

		} else {
			return function(){};
		}

	}();
	function loadTime(e) {		
		try {
			var ruxsent = document.getElementById("ruxsent");
			
			if (ruxsent.value=="false") {
				(new Image()).src = '/timer.jpeg?reqId=efd5827d-e30f-4967-8030-20153813c94a&t1=' + rux_time1 + '&t2=' + (new Date()).getTime() + '&_c=D&pn=ij.presentacion.controllers.VerOfertaController.doGetOfertaEmpleo';
				ruxsent.value = true;
			}			
			
		} catch(err) {
			(new Image()).src = '/timer.jpeg?reqId=efd5827d-e30f-4967-8030-20153813c94a&error=true' + '&_c=D';
		}
	}

	addEvt(window, 'load',loadTime, false);

	// ]]>
</script>

